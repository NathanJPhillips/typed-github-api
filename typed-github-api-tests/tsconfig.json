{
  "compilerOptions": {
    "target": "es5",
    "lib": [ "es2015" ],
    "module": "commonjs",
    "sourceMap": true,
    "removeComments": false,
    "noEmitOnError": true,
    "noImplicitAny": false,
    "strictNullChecks": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true
  },
  "exclude": [
    "node_modules"
  ]
}
